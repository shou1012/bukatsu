<form action="/edit" method="POST" enctype="multipart/form-data">
	<input type="file" name="profile_url"><br>
	<div class=form-group><label for="name">名前</label><br>
	<input type="name"  id="name" name="name" value="<%=current_user.name%>">
	</div>
	<div class=form-group><label for="email">メールアドレス</label><br>
	<input type="email"  id="email" name="email" value="<%=current_user.email%>">
	</div>
	<input type="submit" value="更新">
</form>